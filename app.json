{
  "expo": {
    "name": "ChamberConnect",
    "slug": "chamber-connect",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "chamber-connect",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "The modern platform connecting chambers of commerce and local businesses",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.chamberconnect.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.chamberconnect.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-dev-client",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4f3a7f72-48b9-426e-be59-53d778849fdc"
      }
    },
    "owner": "madxent"
  }
}
