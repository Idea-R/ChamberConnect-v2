{
  "expo": {
    "name": "ChamberConnect",
    "slug": "chamber-connect",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "chamberconnect",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "The modern platform connecting chambers of commerce and local businesses",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.chamberconnect.app"
    },
    "android": {
      "package": "com.chamberconnect.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
