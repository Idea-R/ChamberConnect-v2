{
  "name": "chamber-connect",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": ["immer", "@lucide/lab", "@supabase/mcp-server-postgrest", "@supabase/mcp-server-supabase", "@supabase/mcp-utils"],
        "listUnknownPackages": false
      }
    }
  },
  "scripts": {
    "dev": "EXPO_NO_TELEMETRY=1 expo start",
    "dev:web": "EXPO_NO_TELEMETRY=1 expo start --web",
    "dev:ios": "EXPO_NO_TELEMETRY=1 expo start --ios",
    "dev:android": "EXPO_NO_TELEMETRY=1 expo start --android",
    "build:web": "expo export --platform web",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android",
    "preview": "expo export --platform web && npx serve dist",
    "lint": "expo lint",
    "lint:fix": "expo lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "analyze": "npx @next/bundle-analyzer",
    "db:generate": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > types/supabase.ts",
    "db:migrate": "supabase migration up",
    "db:reset": "supabase db reset",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.2.3",
    "@expo-google-fonts/poppins": "^0.2.3",
    "@expo/metro-runtime": "~5.0.4",
    "@expo/vector-icons": "^14.1.0",
    "@lucide/lab": "^0.1.2",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@shopify/flash-list": "1.7.6",
    "@supabase/mcp-server-postgrest": "^0.1.0",
    "@supabase/mcp-server-supabase": "^0.4.5",
    "@supabase/mcp-utils": "^0.2.1",
    "@supabase/supabase-js": "^2.39.3",
    "expo": "^53.0.0",
    "expo-blur": "~14.1.3",
    "expo-camera": "~16.1.5",
    "expo-constants": "~17.1.3",
    "expo-dev-client": "~5.2.1",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.3",
    "expo-linear-gradient": "~14.1.3",
    "expo-linking": "~7.1.3",
    "expo-router": "~5.1.0",
    "expo-splash-screen": "~0.30.6",
    "expo-status-bar": "~2.2.2",
    "expo-symbols": "~0.4.3",
    "expo-system-ui": "~5.0.5",
    "expo-web-browser": "~14.2.0",
    "immer": "^10.1.1",
    "lucide-react-native": "^0.475.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.4",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.29.0",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "eslint": "^9.29.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "patch-package": "^8.0.0",
    "typescript": "~5.8.3"
  }
}
